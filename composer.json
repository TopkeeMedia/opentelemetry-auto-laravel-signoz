{
    "name": "topkee-media/opentelemetry-auto-laravel-signoz",
    "description": "this is a laravel opentelemetry for signoz",
    "type": "library",
    "require": {
        "open-telemetry/exporter-otlp": "1.0",
        "open-telemetry/sdk": "1.0",
        "open-telemetry/transport-grpc": "1.0"
    },
    "authors": [
        {
            "name": "sherlockliu",
            "email": "hancock.liu@gmail.com"
        }
    ],
    "autoload":{
        "files":[
            "src/_register.php"
        ],
        "psr-4":{ 
            "OpenTelemetry\\Contrib\\Instrumentation\\Laravel\\": "src"
        }
    },
    "config": {
        "mini-stable": true,
        "version": "1.0.0"
    }
}
