{
  "name": "topkee-media/opentelemetry-auto-laravel-signoz",
  "description": "this is a laravel opentelemetry for signoz",
  "type": "library",
  "license": "MIT",
  "require": {
    "laravel/framework": "^8.40",
    "nyholm/psr7": "^1.8",
    "open-telemetry/exporter-otlp": "1.0.4",
    "open-telemetry/sdk": "1.0.5",
    "open-telemetry/transport-grpc": "1.0.0",
    "symfony/http-client": "v6.0.*"
  },
  "authors": [
    {
      "name": "sherlockliu",
      "email": "hancock.liu@gmail.com"
    }
  ],
  "autoload": {
    "files": [
      "src/_register.php"
    ],
    "psr-4": {
      "OpenTelemetry\\Contrib\\Instrumentation\\Laravel\\": "src"
    }
  },
  "config": {
    "mini-stable": true,
    "version": "1.0.0",
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
